{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "123adfdemo"
		},
		"LinkedServiceStorageAccount_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedServiceStorageAccount'"
		},
		"RestServiceSuccFact_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestServiceSuccFact'"
		},
		"HttpLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://www.dneonline.com/calculator.asmx"
		},
		"RestServiceSuccFact_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api2.successfactors.eu/odata/v2"
		},
		"RestServiceSuccFact_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "SPECADMSF@specSF2DEV"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "123adfdemo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedServiceStorageAccount_connectionString": {
						"value": "[parameters('LinkedServiceStorageAccount_connectionString')]"
					},
					"RestServiceSuccFact_password": {
						"value": "[parameters('RestServiceSuccFact_password')]"
					},
					"HttpLinkedService_properties_typeProperties_url": {
						"value": "[parameters('HttpLinkedService_properties_typeProperties_url')]"
					},
					"RestServiceSuccFact_properties_typeProperties_url": {
						"value": "[parameters('RestServiceSuccFact_properties_typeProperties_url')]"
					},
					"RestServiceSuccFact_properties_typeProperties_userName": {
						"value": "[parameters('RestServiceSuccFact_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "123adfdemo_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', '123adfdemo_ArmTemplate_0')]"
			]
		}
	]
}